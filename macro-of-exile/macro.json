{
  "Actions": [
    {
      "$type": "InitializeContextFromFile",
      "Id": "0",
      "FilePath": "context.json",
      "OnSuccess": "enableBreaking"
    },
    {
      "$type": "Break",
      "Id": "enableBreaking",
      "Key": 119,
      "OnSuccess": "takeItem"
    },
    {
      "$type": "SingleClick",
      "Id": "takeItem",
      "X": { "Expression": "$EqFirstSlotX + $ItemCounter * $EqDelta" },
      "Y": { "Expression": "$EqFirstSlotY" },
      "Button": 1,
      "OnSuccess": "putItemIntoStash"
    },
    {
      "$type": "SingleClick",
      "Id": "putItemIntoStash",
      "X": { "Expression": "$ItemX" },
      "Y": { "Expression": "$ItemY" },
      "Button": 1,
      "OnSuccess": "takeAlt"
    },
    {
      "$type": "SingleClick",
      "Id": "takeAlt",
      "X": { "Expression": "$AltX" },
      "Y": { "Expression": "$AltY" },
      "Button": 2,
      "OnSuccess": "useAlt"
    },
    {
      "$type": "SingleClick",
      "Id": "useAlt",
      "X": { "Expression": "$ItemX" },
      "Y": { "Expression": "$ItemY" },
      "Button": 1,
      "OnSuccess": "checkMS"
    },
    {
      "$type": "Copy",
      "Id": "checkMS",
      "X": { "Expression": "$ItemX" },
      "Y": { "Expression": "$ItemY" },
      "Resolver": {
        "$type": "RegexOnClipboard",
        "Regex": "Movement Speed"
      },
      "OnSuccess": "takeItemFromStash",
      "OnFailure": "checkOpenPrefix"
    },
    {
      "$type": "Copy",
      "Id": "checkOpenPrefix",
      "X": { "Expression": "$ItemX" },
      "Y": { "Expression": "$ItemY" },
      "Resolver": {
        "$type": "RegexOnClipboard",
        "Regex": "Prefix"
      },
      "OnSuccess": "takeAlt",
      "OnFailure": "takeAug"
    },
    {
      "$type": "SingleClick",
      "Id": "takeAug",
      "X": { "Expression": "$AugX" },
      "Y": { "Expression": "$AugY" },
      "Button": 2,
      "OnSuccess": "useAug"
    },
    {
      "$type": "SingleClick",
      "Id": "useAug",
      "X": { "Expression": "$ItemX" },
      "Y": { "Expression": "$ItemY" },
      "Button": 1,
      "OnSuccess": "checkMS"
    },
    {
      "$type": "SingleClick",
      "Id": "takeItemFromStash",
      "X": { "Expression": "$ItemX" },
      "Y": { "Expression": "$ItemY" },
      "Button": 1,
      "OnSuccess": "putItemIntoEq"
    },
    {
      "$type": "SingleClick",
      "Id": "putItemIntoEq",
      "X": { "Expression": "$EqFirstSlotX + $ItemCounter * $EqDelta" },
      "Y": { "Expression": "$EqFirstSlotY" },
      "Button": 1,
      "OnSuccess": "nextItem"
    },
    {
      "$type": "SetVariable",
      "Id": "nextItem",
      "Variable": "ItemCounter",
      "Value": { "Expression": "$ItemCounter + 1" },
      "OnSuccess": "takeItem"
    },
    {
      "$type": "SingleClick",
      "Id": "end",
      "X": { "Expression": "$ItemX" },
      "Y": { "Expression": "$ItemY" },
      "Button": 0,
      "IsLast": true
    }
  ]
}